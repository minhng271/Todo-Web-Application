{
    "devDependencies": {
      "@babel/eslint-parser": "^7.19.1",
      "eslint": "^8.2.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.25.2",
      "eslint-plugin-prettier": "^4.2.1",
      "husky": "^8.0.1",
      "parcel": "^2.7.0",
      "parcel-bundler": "^1.3.1",
      "prettier": "2.7.1"
    },
    "scripts": {
      "start": "parcel src/index.html --port 5000",
      "start-no-cache": "del dist && parcel src/index.html --port 5000 --no-cache",
      "start-db": "json-server --watch data/db.json --host 127.0.0.1",
      "build": "parcel build src/index.html",
      "lint:eslint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
      "fix:eslint": "eslint --fix --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx"
    },
    "dependencies": {
      "json-server": "^0.17.0"
    }
  }
  